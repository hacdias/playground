{
	"name": "notes",
	"displayName": "notes",
	"publisher": "hacdias",
	"description": "",
	"repository": {
		"type": "git",
		"url": "https://github.com/hacdias/vscode-notes"
	},
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.44.0"
	},
	"categories": [
		"Snippets",
		"Keymaps",
		"Other"
	],
	"activationEvents": [
		"onLanguage:markdown",
		"onCommand:notes.newNote",
		"onCommand:notes.resetBackLinks",
		"onCommand:notes.insertDate"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "notes.newNote",
				"title": "New Note"
			},
			{
				"command": "notes.insertDate",
				"title": "Insert Current Date"
			},
			{
				"command": "notes.resetBackLinks",
				"title": "Reset Back Links Cache"
			}
		],
		"keybindings": [
			{
				"mac": "cmd+n",
				"win": "ctrl+n",
				"linux": "ctrl+n",
				"key": "ctrl+n",
				"command": "notes.newNote"
			}
		],
		"views": {
			"explorer": [
				{
					"id": "backlinks",
					"name": "Backlinks"
				}
			]
		},
		"viewsWelcome": [
			{
				"view": "backlinks",
				"contents": "No backlinks for this note were found."
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/fs-extra": "^8.1.0",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.44.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"fs-extra": "^9.0.0",
		"slugify": "^1.4.0"
	}
}
